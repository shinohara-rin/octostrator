{
  "name": "octostrator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "octostrator": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@vitest/coverage-v8": "^4.0.16",
    "proper-lockfile": "^4.1.2",
    "zod": "^4.2.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,json,md,yaml,yml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,cjs,mjs}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/proper-lockfile": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
